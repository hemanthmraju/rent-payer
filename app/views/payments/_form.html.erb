<%= form_for @payment do |f| %>
<div class="col-lg-offset-1">
              <%= f.hidden_field :user_id %>
              <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :rent ,:class => 'col-xs-4'%>
                    <div class="col-xs-3">
                    <%= f.text_field :rent , :placeholder => "Rent"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :parking ,:class => 'col-xs-4'%>
                    <div class="col-xs-3">
                    <%= f.text_field :parking , :placeholder => "parking"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :late_fee ,:class => 'col-xs-4'%>
                    <div class="col-xs-3">
                    <%= f.text_field :late_fee , :placeholder => "late_fee"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :other,:class => 'col-xs-4' %>
                    <div class="col-xs-3">
                    <%= f.text_field :other , :placeholder => "other"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :application_fee,:class => 'col-xs-4' %>
                    <div class="col-xs-3">
                    <%= f.text_field :application_fee , :placeholder => "application_fee"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :deposit,:class => 'col-xs-4' %>
                    <div class="col-xs-3">
                    <%= f.text_field :deposit , :placeholder => "deposit"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :move_in_other,:class => 'col-xs-4' %>
                    <div class="col-xs-3">
                    <%= f.text_field :move_in_other , :placeholder => "move_in_other"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :promo_code ,:class => 'col-xs-4'%>
                    <div class="col-xs-3">
                    <%= f.text_field :promo_code , :placeholder => "promo_code"%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <div class="row control-group">
                  <div class="form-group col-xs-12  ">
                    <%= f.label :total ,:class => 'col-xs-4'%>
                    <div class="col-xs-3">
                    <%= f.text_field :total , :placeholder => "total", :disabled=>true%>
                    </div>
                    <p class="help-block text-danger"></p>
                  </div>
               </div>
               <br>
                  <div id="success"></div>
                  <div class="row">
                      <div class="form-group col-xs-12 col-lg-offset-7">
                          <%= f.submit "Continue", :class => "btn btn-warning" %>
                      </div>
                  </div>
                  </div>
              <%end%>